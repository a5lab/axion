#{{ ansible_managed }}
[Unit]
Description=tabr
Wants=postgresql.service
After=network.target local-fs.target postgresql.service

[Service]
Type=forking
User={{ app__user }}
WorkingDirectory={{ app__current_path }}
SyslogIdentifier=tabr
TimeoutSec=5min
Restart=on-failure
RestartSec=5s
ExecStart=java -jar {{ app__current_path }}/target/tabr-0.0.1-SNAPSHOT.jar

[Install]
WantedBy=multi-user.target
